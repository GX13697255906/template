<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dms.cloud.biz302.server.mapper.UnitDeemedCostMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dms.cloud.biz302.client.model.entity.UnitDeemedCost">
        <id column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="power_plant_code" property="powerPlantCode" />
        <result column="unit_code" property="unitCode" />
        <result column="power_plant_name" property="powerPlantName" />
        <result column="unit_name" property="unitName" />
        <result column="coldstarting_cost" property="coldstartingCost" />
        <result column="warmstarting_cost" property="warmstartingCost" />
        <result column="hotstarting_cost" property="hotstartingCost" />
        <result column="unloaded_cost" property="unloadedCost" />
        <result column="energy_cost" property="energyCost" />
        <result column="effective_time" property="effectiveTime" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        update_time,
        id, power_plant_code, unit_code, power_plant_name, unit_name, coldstarting_cost, warmstarting_cost, hotstarting_cost, unloaded_cost, energy_cost, effective_time, status
    </sql>

    <!--自定义关联查询-->
    <!-- 表名 -->
    <sql id="t_name">biz_unit_deemed_cost</sql>
    <!-- 别名 -->
    <sql id="t_alias">biz_unit_deemed_cost as cost</sql>
    <!--外键表关联定义 -->
    <sql id="join"></sql>

    <!-- 自定义查询单个实体 -->
    <select id="getEntityMap" resultType="com.dms.cloud.common.mybatis.EntityMap">
        SELECT * FROM
        <include refid="t_alias"/>
        <include refid="join"/>
        ${ew.customSqlSegment}
    </select>

    <!-- 自定义关联查询分页 -->
    <select id="pageList" resultType="com.dms.cloud.common.mybatis.EntityMap">
        select ${ew.select} FROM
        <include refid="t_alias"/>
        <include refid="join"/>
        ${ew.customSqlSegment}
    </select>

    <select id="selectByEffectiveTime" resultMap="BaseResultMap" >
        select * FROM biz_unit_deemed_cost
        WHERE status = 0
        and STR_TO_DATE(effective_time, "%Y%m%d") &gt;= #{effectiveTime}
    </select>
</mapper>
